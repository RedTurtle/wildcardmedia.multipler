<link href="${view/get_resource_css}" rel="stylesheet">
<div class="wcvideo-container"
    tal:define="util context/@@wcmedia-utils;
                settings util/settings;
                video nocall: video|context;
                video_url view/getVideoURL|nothing;
                poster_url view/getPosterURL;">
    <div id="multipler-video"
         data-video-data='{"videoUrl": "${video_url}", "posterUrl": "${poster_url}", "language": "${view/get_language}"}'/>
    <script src="${view/get_resource_js}"></script>
</div>
